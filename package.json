{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for Voya Code",
  "author": "Voya",
  "engines": {
    "node": ">8.11.3"
  },
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest-full.json --runInBand",
    "test:unit": "cross-env NODE_ENV=test jest --config ./test/jest-unit.json --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --runInBand",
    "test:cov": "cross-env NODE_ENV=test jest --config ./test/jest-full.json --runInBand --coverage",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n CommentMigration"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/passport": "^1.1.0",
    "@nestjs/typeorm": "^5.0.2",
    "@types/bcrypt": "^2.0.0",
    "@types/mailgun-js": "^0.16.0",
    "@types/morgan": "^1.7.35",
    "@types/validator": "^9.4.1",
    "bcrypt": "^3.0.0",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.8.5",
    "escape-html": "^1.0.3",
    "fastify-formbody": "^2.0.0",
    "helmet": "^3.12.1",
    "jsonwebtoken": "^8.3.0",
    "mailgun-js": "^0.20.0",
    "moment-timezone": "^0.5.21",
    "morgan": "^1.9.0",
    "node-cache": "^4.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "rss": "^1.2.2",
    "rxjs": "^6.0.0",
    "typeorm": "^0.2.7",
    "typescript": "^2.9.2",
    "url-crypt": "^1.2.1",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/escape-html": "0.0.20",
    "@types/express": "^4.0.39",
    "@types/helmet": "0.0.38",
    "@types/jest": "^21.1.8",
    "@types/moment-timezone": "^0.5.6",
    "@types/node": "^9.3.0",
    "@types/node-cache": "^4.1.1",
    "@types/passport-jwt": "^3.0.1",
    "@types/rss": "0.0.28",
    "@types/supertest": "^2.0.4",
    "cross-env": "^5.2.0",
    "jest": "^23.3.0",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.1.0",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  }
}
