{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for Voya Code",
  "main": "server.js",
  "scripts": {
    "start": "set PORT=8000 && concurrently \"nodemon --watch ./ server.js --watch ./.config\" \"npm run open-browser\" ",
    "open-browser": "browser-sync start --proxy http://localhost:8000 --reload-delay 200 --browser \"chrome\" --files ../public/index.html",
    "test": "cross-env NODE_ENV=test && mocha",
    "test-cover": "istanbul cover _mocha $(find \"*.spec.js\" -not -path \"./node_modules/*\")"
  },
  "author": "Voya",
  "license": "ISC",
  "engines": {
    "node": ">=7.0.0"
  },
  "env": [
    "node",
    "mocha"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "~1.5.2",
    "chai": "^3.5.0",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "mailgun-js": "^0.13.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "morgan": "^1.2.3",
    "node-cache": "^4.1.1",
    "pg": "^6.1.0",
    "pg-promise": "^5.9.7",
    "request": "^2.81.0",
    "rss": "^1.2.2",
    "url-crypt": "^1.2.1"
  },
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.6.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.14.0"
  }
}
